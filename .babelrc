{
  "presets": [
    ["es2015", { "modules": false }]
  ],
  "plugins": [
    ["transform-runtime", {
      "regenerator": false
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        ["istanbul", {
          "exclude": [
            "test/**/*.js",
            "test/**/*.ts"
          ]
        }]
      ]
    }
  }
}
